generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Media {
  id           Int     @id @default(autoincrement())
  title        String
  thumbnail    Json
  year         Int
  category     String
  rating       String
  isBookmarked Boolean @default(false)
  isTrending   Boolean @default(false)
}
